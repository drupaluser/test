<?php

/**
 * @file
 * Panes with body for text and the possibility to add a radio to your site. For Panels
 */

/**
 * Implements hook_ctools_plugin_directory().
 */
function panes_ctools_plugin_directory($owner, $plugin_type) {
  if ($owner == 'ctools' && $plugin_type == 'content_types') {
    return 'plugins/' . $plugin_type;
  }
}
